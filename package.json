{
  "name": "git-frisky",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "workspaces": [
    "apps/ui",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm -C apps/ui dev",
    "typecheck": "pnpm -C apps/ui typecheck && pnpm -C packages/* typecheck",
    "lint": "pnpm -C apps/ui lint",
    "build": "pnpm -C apps/ui build"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.0"
  }
}
